{
  "singleQuote": true,
  "pugFramework": "vue",
  "pugSingleQuote": false,
  "pugSortAttributesBeginning": [
    "^v-for$",
    "^:key$",
    "^v-if$",
    "^v-else-if$",
    "^v-else$",
    "^v-on$",
    "^v-bind$",
    "^ref$",
    "^v-model",
    "^name$",
    "^:?type$",
    "^:value$",
    "^v-text$",
    "^:?label$",
    "^:?src$"
  ],
  "pugSortAttributesEnd": ["^@click", "^@"]
}
